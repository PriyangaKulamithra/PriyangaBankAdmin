@model PriyangaBankAdmin.ViewModels.Kundregister.CustomerItem
@{
    Layout = null;
}
<tr>
    <td class="pl-4">@Model.Id</td>
    <td>@Model.Name</td>
    <td>@Model.NationalId</td>
    <td>@Model.City</td>
    <td class="text-center">@Model.NumberOfOwnAccounts</td>
    <td><a asp-controller="Kundregister" asp-action="Kundbild" asp-route-id="@Model.Id.ToString()">Visa Kundbild</a></td>
</tr>

